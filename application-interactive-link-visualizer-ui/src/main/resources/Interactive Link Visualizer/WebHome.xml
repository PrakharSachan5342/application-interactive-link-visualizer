  <content>{{velocity output="false"}}#set($solrConfig = {'filterQuery': ['type:DOCUMENT'], 'facetFields': ['property.XWiki.TagClass.tags_string', 'wiki', 'space_facet', 'locale', 'author', 'creator', 'date', 'creationdate', 'class', 'mimetype', 'attauthor', 'attdate', 'attsize'], 'facetEnabled': false}){{/velocity}}{{include reference="Main.SolrSearchConfig" /}}{{include reference="Main.SolrSearchMacros" /}}{{velocity}}#macro (displaySearchResults){{html}}
&lt;div id="sigma-container" data-results="$escapetool.xml($jsontool.serialize($searchResponse.results))"&gt;
&lt;div id="top-bar"&gt;
   &lt;div id="search"&gt;
      &lt;i class="fa fa-search"&gt;&lt;/i&gt;
      &lt;input
         type="search"
         id="search-input"
         list="suggestions"
